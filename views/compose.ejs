<%- include("partials/header") -%>

<div class="composeStyle">
  <h1>Compose</h1>
  <form class="" action="/compose" method="post">
    <div class="form-group">
      <label>Title</label>
      <input class="form-control" type="text" name="postTitle" autocomplete="off">
      <label>Post </label>
      <textarea class="form-control" name="postBody" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn2" type="submit" name="button">Publish</button>
  </form>
</div>



<%- include("partials/footer") -%>
